generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ClienteEvento {
    id              Int       @id @default(autoincrement())
    empresa         String
    responsavel     String
    cnpj_cpf        String
    endereco        String
    numero          Int
    bairro          String
    cidade          String
    tipo_Evento     String
    data_evento     DateTime
    contato         String
    email           String
    descricao       String
    data_cadastro   DateTime @default(now())
}