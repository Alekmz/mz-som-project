generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url= env("DATABASE_URL")
}

model Budget {
    id              Int       @id @default(autoincrement())
    responsavel     String
    cnpj_cpf        String
    cidade          String
    tipo_servico    String[]
    data_evento     DateTime
    contato         String
    email           String
    descricao       String
    data_cadastro   DateTime @default(now())
}
